{
  "data_inputs": [
      {
          "name": "first",
          "type": "file",
          "uri": "{basepath}/iraq_hospitals.geojson"
      },
      {
          "type": "process",
          "process": {
              "name": "buffer",
              "data_inputs": [
                  {
                      "type": "file",
                      "uri": "{basepath}/iraq_roads.geojson",
                      "filter": {
                          "type": "motorway"
                      }
                  }
              ],
              "args": {
                  "buffer_size": 10000
              }
          }
      }
  ],
    "output": {
        "type": "file"
    }
}